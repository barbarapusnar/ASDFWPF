        <StackPanel x:Name="vsebnik" HorizontalAlignment="Left"  Margin="20,20,0,0" Grid.Row="1" VerticalAlignment="Top" >
                <!--<local:Tipkovnica  x:Name="mojaTipkovnica"/>-->            
        </StackPanel>

        
    </Grid>
</Window>
